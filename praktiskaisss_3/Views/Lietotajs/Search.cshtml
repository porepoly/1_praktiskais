@model IEnumerable<_1_praktiskais.Models.Lietotaj>
@{
    ViewData["Title"] = "Search";
}

<h1>Search Lietotaji</h1>

<form method="post">
    <input type="text" name="query" placeholder="Ievadi id, vardu vai uzvardu" />
    <button type="submit">Meklet</button>
</form>

@if (Model != null && Model.Any())
{
    <table>
        <tr>
            <th>ID</th>
            <th>Vards</th>
            <th>Uzvards</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Vards</td>
                <td>@item.Uzvards</td>
            </tr>
        }
    </table>
}
